<link rel="stylesheet" href="/style.css"/>
<section class="home">
      
  <div class=" form Signup-form">
      <form id="signupForm" action="#" method="post" onsubmit="return validateSignUpForm()" >
        <h2>Sign Up</h2>
        <div class="mb-3 inputbox">
          
          <input name="name" class="form-control" id="exampleInputName" placeholder="Enter Your Name" required>
          
        </div>
        <div class="mb-3 inputbox">
          
          <input name="Email" class="form-control" id="exampleInputName" placeholder="Enter Your Email" required>
          
        </div>
  
        <div class="mb-3 inputbox">
          <input type="password" class="form-control" id="exampleInputpass" placeholder="Enter Your Password" required>
          
        </div>
  
        <div class="mb-3 inputbox">
          <input type="password" class="form-control" id="exampleInputpass1" placeholder="Confirm Your Password" required>
          
        </div>
  
       <div class="opt-field">
          <span class="checkbox">
            <input type="checkbox" id="check">
            <label for="check">Remember me</label>
  
          </span>
       </div>
       <div  class="mb-3 ">
        <button class="btn btn-primary button">Sign Up </button>
      </div> 
      </form>
  </div>
  </section>

  <script src="/1.js"></script>