<link rel="stylesheet" href="/style.css"/>

<section class="home">
  <div class="form-container ">
  
          <div class="login-form">
              <form id="loginForm" action="#" method = "post" onsubmit="validateLoginForm()">
                <h2>Login</h2>
                <div class="mb-3 inputbox">
                  
                  <input name="Email" class="form-control" id="exampleInputName" placeholder="Enter Your Email" required>
                  
                </div>
  
                <div class="mb-3 inputbox">
                  <input type="password" class="form-control" id="exampleInputpass" placeholder="Enter Your Password" required>
                  
                 
                </div>
               <div class="opt-field">
                  <span class="checkbox">
                    <input type="checkbox" id="check">
                    <label for="check">Remember me</label>
  
                    <a href="#"> Forgot Password?</a>
                  </span>
               </div>
                <div  class="mb-3 ">
                  <button class="btn btn-primary button">Login </button>
                </div>   
              </form>
           </div>

           <script src="/1.js"></script>
